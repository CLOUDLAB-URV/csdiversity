{
  "description": "Sample data structure for Conference Data Visualizer",
  "conferences": ["OSDI", "SOSP", "ASPLOS", "NSDI", "SIGCOMM"],
  "year_range": [2000, 2024],
  "data_structure": {
    "papers": {
      "columns": ["year", "conference", "title", "continent", "authors", "institutions", "countries"],
      "description": "Accepted papers data"
    },
    "citations": {
      "columns": ["paper_id", "cited_by", "continent_distribution", "unknown_count"],
      "description": "Citation data with continental distribution"
    },
    "committee": {
      "columns": ["year", "conference", "member_name", "institution", "country", "continent"],
      "description": "Committee member data"
    },
    "big_tech": {
      "columns": ["year", "conference", "has_big_tech_affiliation", "companies", "academic_institutions"],
      "description": "Big Tech vs Academia analysis"
    }
  }
}


